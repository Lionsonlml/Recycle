@model IEnumerable<RecycleDevices.Models.Token>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.finicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ffin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.USUARIO)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.finicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ffin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USUARIO.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_token">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_token">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_token">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
